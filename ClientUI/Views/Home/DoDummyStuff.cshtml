@model dynamic
@{
    ViewBag.Title = "Dummy studd done";
    // Want to keep this info out of the controller to make controller more understandable
    if (Model.MessagesSent < 5)
    {
        Model.ShowForm = false;
        Model.LeftWidth = 12;
    }
    else
    {
        Model.ShowForm = true;
        Model.LeftWidth = 7;
        Model.RightWidth = 5;
    }
}


